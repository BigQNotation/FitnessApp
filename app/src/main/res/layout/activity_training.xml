<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/training_previous_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Previous"
        tools:layout_constraintRight_toRightOf="parent"
        tools:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteX="-7dp"
        tools:layout_editor_absoluteY="27dp" />

    <TextView
        android:id="@+id/training_header_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Training"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@id/training_previous_button"
        app:layout_constraintEnd_toStartOf="@id/training_next_button"
        app:layout_constraintStart_toEndOf="@id/training_previous_button"
        app:layout_constraintTop_toTopOf="@id/training_previous_button" />

    <Button
        android:id="@+id/training_next_button"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Next"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="27dp" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/training_header_textview">

